CXXFLAGS = -Wall -s -O2 -lglut -lGL -lGLU

all: main
clean:
	rm -f *.o
cleanall: clean
	rm -f main
rebuild: clean all
debug: all
	./main

main: main.o a.o b.o
	$(CXX) $(CXXFLAGS) -o $@ $^
main.o: main.cc a.h b.h
	$(CXX) $(CXXFLAGS) -c -o $@ $<
a.o: a.cc a.h b.h
	$(CXX) $(CXXFLAGS) -c -o $@ $<
b.o: b.cc b.h a.h
	$(CXX) $(CXXFLAGS) -c -o $@ $<

